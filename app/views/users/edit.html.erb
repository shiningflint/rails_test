<% content_for(:title) { '写真アップロード' } %>
<div>
  <%# <%= form_with model: @new_user_photo, url: photos_path, method: :post do |form| %1> %>
  <%= form_with url: photos_path, method: :post do |form| %>
    <%= form.hidden_field :user_id, value: params[:id] %>
    <%= form.label :title, 'タイトル' %>
    <%= form.text_field :title %>

    <br />
    <%= form.label :photo_file, '画像ファイル' %>
    <%= form.file_field :photo_file %>

    <br />
    <%= form.submit 'アップロード' %>
    <%= link_to 'キャンセル', user_path(id: params[:id]) %>
  <% end %>
</div>
